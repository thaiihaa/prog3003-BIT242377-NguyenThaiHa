package BaiTap;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;
import java.util.function.Function;

public class Bai4 {
    public static void main(String[] args) {
        List<String> moneyStrings = Arrays.asList("$10", "$20", "$50");
        
        Function<String, Integer> parseMoney = s -> Integer.parseInt(s.replace("$", ""));

        List<Integer> moneyIntegers = new ArrayList<>();
        for (String s : moneyStrings) {
            moneyIntegers.add(parseMoney.apply(s));
        }

        System.out.println("Danh sách số nguyên: " + moneyIntegers);
    }
}
